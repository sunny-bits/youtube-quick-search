{
  "manifest_version": 3,
  "name": "YouTube Quick Search",
  "version": "1.0.0",
  "description": "An extension that allows you to search on YouTube using the > prefix in the address bar.",
  "author": {
    "name": "Sunnybits",
    "website": "https://sunnybits.com",
    "github": "https://github.com/sunny-bits"
  },
  "background": {
    "service_worker": "background.js"
  },
  "omnibox": {
    "keyword": ">"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "64": "icon64.png",
    "128": "icon128.png"
  }
}